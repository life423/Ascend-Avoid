{
  "name": "cross-the-box-game",
  "version": "1.0.0",
  "description": "A simple canvas game where the player needs to cross the box without hitting obstacles",
  "main": "index.html",
  "type": "module",
  "scripts": {
    "setup": "node install.cjs",
    "postinstall": "node install.cjs",
    "dev": "npx http-server src -o -p 8080",
    "start": "npx http-server src -o -p 8080",
    "build": "node build.cjs",
    "serve:dist": "npx http-server dist -o -p 8080",
    "lint:js": "eslint src/js/**/*.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "game",
    "canvas",
    "javascript",
    "html5",
    "responsive"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "babel-jest": "^29.5.0",
    "eslint": "^8.23.0",
    "http-server": "^14.1.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "terser": "^5.14.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
